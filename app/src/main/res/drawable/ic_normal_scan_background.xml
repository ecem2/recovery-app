<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#2D34FE"
                android:endColor="#1685FC"
                android:type="linear"
                android:angle="90"/>
            <corners android:radius="30dp" />
        </shape>
    </item>
</layer-list>